<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="TextInputView">
		<attr name="label" format="string" />
		<attr name="placeholder" format="string" />
		<attr name="text" format="string" />
		<attr name="textSize" format="dimension" />
		<attr name="textPaddingStart" format="dimension" />
		<attr name="textPaddingTop" format="dimension" />
		<attr name="textPaddingEnd" format="dimension" />
		<attr name="textPaddingBottom" format="dimension" />
		<attr name="textPaddingHorizontal" format="dimension" />
		<attr name="textPaddingVertical" format="dimension" />
		<attr name="priorityUserInput" format="boolean" />
		<attr name="inputEndIconMode" format="enum">
			<enum name="none" value="0" />
			<enum name="password_toggle" value="1" />
			<enum name="clear_text" value="2" />
			<enum name="dropdown_menu" value="3" />
		</attr>
	</declare-styleable>
</resources>
